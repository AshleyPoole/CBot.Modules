<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <PackageId>CBot.Modules.Cloudflare</PackageId>
    <PackageVersion>1.0.4</PackageVersion>
    <Authors>Ashley Poole</Authors>
    <Description>Cloudflare module for CBot framework</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Add ability to get Cloudflare Logs for RAY ID</PackageReleaseNotes>
    <PackageTags>CBot.Modules CBot Cloudflare CloudflareCache csharpbot chatops chatbot bot</PackageTags>
    <PackageProjectUrl>https://github.com/AshleyPoole/CBot.Modules</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AshleyPoole/CBot.Modules</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="CBot" Version="1.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.1.1" />
  </ItemGroup>

</Project>
